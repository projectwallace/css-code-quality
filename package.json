  "version": "0.0.0",
  "repository": "git@github.com:projectwallace/css-code-quality.git",
  "author": {
    "email": "bart@projectwallace.com",
    "name": "Bart Veneman"
  },
  "keywords": [
    "css",
    "codequality",
